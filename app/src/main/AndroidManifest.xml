<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abrahamnews">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application

        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme"

        >

        <activity android:name=".Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".wiFi.No_item_internet_image" />
        <activity android:name=".search.SearchActivity" />
        <activity android:name=".setting.Setting" />
        <activity
            android:name=".detail.NewsDetailActivity"
            android:label=""
            android:screenOrientation="portrait" />
        <activity android:name=".list.NewsListActivity" />
        <activity android:name=".Factory">

        </activity>

    </application>

</manifest>